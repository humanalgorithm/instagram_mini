<div class="form-container-outer-img">
	<div class="form-container-inner-img">

		<%= form_for @image_post, url: user_image_posts_path, html: { multipart: true } do |form| %>
		<div class = "field">
			<%= form.label :img %>
			<br>
			<%= form.file_field :img%>
		</div>
		<br>
		<div id="field">
		<%= form.submit "Upload image", :id => "upload-img"%>
		</div>
		<% end %>

	</div>
</div>
